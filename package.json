{
    "name": "@yandex-cloud/uikit",
    "version": "2.5.0",
    "description": "Yandex.Cloud UI components",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/yandex-cloud/uikit"
    },
    "main": "build/cjs/index.js",
    "module": "build/esm/index.js",
    "types": "build/esm/index.d.ts",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "files": [
        "build",
        "styles"
    ],
    "scripts": {
        "prepare": "husky install",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "clean": "gulp clean",
        "build": "gulp",
        "start": "start-storybook -p 7007",
        "build-storybook": "build-storybook -c .storybook -o storybook-static",
        "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
        "lint:styles": "stylelint '{styles,src}/**/*.scss'",
        "lint:prettier": "prettier --check '**/*.md'",
        "lint": "run-p lint:*",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run build"
    },
    "dependencies": {
        "@popperjs/core": "2.11.2",
        "bem-cn-lite": "4.0.0",
        "focus-trap": "6.7.2",
        "lodash": "4.17.21",
        "react-copy-to-clipboard": "5.0.4",
        "react-popper": "2.2.5",
        "react-sortable-hoc": "2.0.0",
        "react-virtualized-auto-sizer": "1.0.6",
        "react-window": "1.8.6",
        "resize-observer-polyfill": "1.5.1",
        "tslib": "2.3.1",
        "utility-types": "3.10.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^15.0.0",
        "@commitlint/config-conventional": "^15.0.0",
        "@storybook/addon-essentials": "^6.4.18",
        "@storybook/preset-scss": "^1.0.3",
        "@storybook/react": "^6.4.18",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^12.1.3",
        "@testing-library/user-event": "^14.1.1",
        "@types/jest": "^27.4.0",
        "@types/lodash": "^4.14.177",
        "@types/react": "^16.14.23",
        "@types/react-copy-to-clipboard": "^5.0.2",
        "@types/react-dom": "^16.9.14",
        "@types/react-virtualized-auto-sizer": "^1.0.1",
        "@types/react-window": "^1.8.5",
        "@yandex-cloud/eslint-config": "^1.0.0",
        "@yandex-cloud/i18n": "^0.4.0",
        "@yandex-cloud/prettier-config": "^1.0.0",
        "@yandex-cloud/stylelint-config": "^1.0.0",
        "@yandex-cloud/tsconfig": "^1.0.0",
        "css-loader": "^5.2.7",
        "eslint": "^8.8.0",
        "eslint-plugin-testing-library": "^5.5.1",
        "gulp": "^4.0.2",
        "gulp-cli": "^2.3.0",
        "gulp-dart-sass": "^1.0.2",
        "gulp-replace": "^1.1.3",
        "gulp-typescript": "^5.0.1",
        "husky": "^7.0.4",
        "jest": "^27.4.7",
        "jest-transform-css": "^3.0.0",
        "nano-staged": "^0.6.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.6",
        "prettier": "2.4.1",
        "react": "^16.14.0",
        "react-docgen-typescript": "^2.1.1",
        "react-dom": "^16.14.0",
        "rimraf": "^3.0.2",
        "sass": "^1.49.7",
        "sass-loader": "^10.2.1",
        "style-loader": "^2.0.0",
        "stylelint": "^14.3.0",
        "ts-jest": "^27.1.3",
        "typescript": "^4.5.5",
        "webpack": "^4.46.0"
    },
    "peerDependencies": {
        "@yandex-cloud/i18n": "^0.4.0",
        "react": "^16.0.0",
        "react-dom": "^16.0.0"
    },
    "nano-staged": {
        "*.{scss}": [
            "stylelint --fix --quiet"
        ],
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix --quiet"
        ],
        "*.md": [
            "prettier --write"
        ]
    }
}
